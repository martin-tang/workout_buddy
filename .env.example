# OpenAI API Key (Required)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Server Port (Optional)
PORT=3000

# MongoDB Connection (Optional - app works without MongoDB in demo mode)
# Format: mongodb://localhost:27017/workout_app or your MongoDB Atlas URI
MONGO_URI=mongodb://localhost:27017/workout_app

# JWT Secret (Required for authentication features)
JWT_SECRET=your_jwt_secret_key_here
